  "scripts": {
    "start": "nodemon --exec babel-node -- app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && babel . -d dist --ignore node_modules --out-dir dist && cp -R views dist",
    "build-test": "rm -rf dist && babel . -d dist --ignore node_modules --out-dir dist && cp -R views dist",
    "testing": "rm -rf babel . -d dist --out-dir dist && cp -R views dist && cp -R config/ssl dist/config && cp -R config/swagger dist/config",
    "serve": "NODE_ENV=production pm2 start dist/app.js --name=app",
    "ri": "rm -rf node_modules && npm install",
    "ngrok": "ngrok http 6003",
    "dev": "babel-node -- app.js"
  },
